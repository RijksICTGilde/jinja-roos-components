{
  "name": "c-button",
  "description": "RVO button component with Utrecht styling and comprehensive variants",
  "attributes": [
    {
      "name": "kind",
      "description": "Visual style variant of the button",
      "value": {
        "required": false,
        "type": "enum",
        "enum": [
          "primary",
          "secondary",
          "tertiary",
          "quaternary",
          "subtle",
          "warning-subtle",
          "warning"
        ],
        "default": "primary"
      }
    },
    {
      "name": "size",
      "description": "Size of the button",
      "value": {
        "required": false,
        "type": "enum",
        "enum": [
          "xs",
          "sm",
          "md"
        ],
        "default": "md"
      }
    },
    {
      "name": "label",
      "description": "Button text label",
      "value": {
        "required": false,
        "type": "string"
      }
    },
    {
      "name": "disabled",
      "description": "Whether the button is disabled",
      "value": {
        "required": false,
        "type": "boolean",
        "default": false
      }
    },
    {
      "name": "busy",
      "description": "Whether the button shows a loading/busy state",
      "value": {
        "required": false,
        "type": "boolean",
        "default": false
      }
    },
    {
      "name": "show-icon",
      "description": "Icon placement in button",
      "value": {
        "required": false,
        "type": "enum",
        "enum": [
          "no",
          "before",
          "after"
        ],
        "default": "no"
      }
    },
    {
      "name": "icon",
      "description": "Icon name from RVO icon set",
      "value": {
        "required": false,
        "type": "string"
      }
    },
    {
      "name": "full-width",
      "description": "Whether button takes full width",
      "value": {
        "required": false,
        "type": "boolean",
        "default": false
      }
    },
    {
      "name": "type",
      "description": "HTML button type",
      "value": {
        "required": false,
        "type": "enum",
        "enum": [
          "button",
          "submit",
          "reset"
        ],
        "default": "button"
      }
    },
    {
      "name": "class",
      "description": "Additional CSS classes",
      "value": {
        "required": false,
        "type": "string"
      }
    },
    {
      "name": "click",
      "description": "JavaScript function to call on click event",
      "value": {
        "required": false,
        "type": "string"
      }
    },
    {
      "name": "mousedown",
      "description": "JavaScript function to call on mousedown event",
      "value": {
        "required": false,
        "type": "string"
      }
    },
    {
      "name": "mouseup",
      "description": "JavaScript function to call on mouseup event",
      "value": {
        "required": false,
        "type": "string"
      }
    },
    {
      "name": "focus",
      "description": "JavaScript function to call on focus event",
      "value": {
        "required": false,
        "type": "string"
      }
    },
    {
      "name": "blur",
      "description": "JavaScript function to call on blur event",
      "value": {
        "required": false,
        "type": "string"
      }
    },
    {
      "name": "keydown",
      "description": "JavaScript function to call on keydown event",
      "value": {
        "required": false,
        "type": "string"
      }
    },
    {
      "name": "keyup",
      "description": "JavaScript function to call on keyup event",
      "value": {
        "required": false,
        "type": "string"
      }
    }
  ]
}